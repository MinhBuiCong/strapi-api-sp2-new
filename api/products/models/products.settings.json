{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "products"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "feature": {
      "type": "enumeration",
      "enum": [
        "NONE",
        "FEATURED_OFFER",
        "FEATURED_SELLER"
      ]
    },
    "image": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "price": {
      "type": "integer"
    }
  }
}
